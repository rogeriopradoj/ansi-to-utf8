  "repository": "https://github.com/rogeriopradoj/ansi-to-utf8",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.1.0",
    "iconv-lite": "0.4.13"
  }
}
