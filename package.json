{
  "name": "ansi-to-utf8",
  "main": "./lib/ansi-to-utf8",
  "version": "0.3.0",
  "description": "AnsiToUTF8 package for editing and saving files encoded in ANSI (Western) Shift_JIS, EUC-JP, CP932, GBK, BIG5, BIG5-HKSCS, EUC-KR etc",
  "activationCommands": {
    "atom-workspace": [
      "ansi-to-utf8:ansi",
      "ansi-to-utf8:shift_jis",
      "ansi-to-utf8:euc-jp",
      "ansi-to-utf8:gbk",
      "ansi-to-utf8:big5",
      "ansi-to-utf8:big5-hkscs",
      "ansi-to-utf8:euc-kr",
      "ansi-to-utf8:utf-8"
    ]
  },
  "repository": "https://github.com/rogeriopradoj/ansi-to-utf8",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3",
    "iconv-lite": "0.4.8"
  }
}
